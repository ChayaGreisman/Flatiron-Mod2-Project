<%= form_for @user do |f| %>
    <%= f.label :username %><br>
    <%= f.text_field :username, class:'form-element'%><br/>
  
    <%= f.label :name %><br>
    <%= f.text_field :name, class:'form-element'%><br/>
  
    <%= f.label :age %><br>
    <%= f.number_field :age, class:'form-element'%><br/>
  
    <%= f.submit class:'submit-button create-edit-button' %>
  <% end %>